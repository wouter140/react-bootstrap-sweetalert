{
  "name": "react-bootstrap-sweetalert",
  "version": "4.4.1",
  "description": "A variant of sweetalert for use with React and Bootstrap",
  "main": "dist/index.js",
  "scripts": {
    "preversion": "yarn build",
    "postversion": "git push && git push --tags && npm publish",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "build": "yarn lint && rm -rf ./dist && tsc && yarn inject-css",
    "build-win": "yarn lint && (if exist dist (rmdir dist /S /Q)) && tsc && yarn inject-css",
    "demo-build-run": "yarn build && yarn build-demo && yarn start-demo && yarn open",
    "demo-build-run-win": "yarn build-win && yarn build-demo && yarn start-demo && yarn open",
    "build-demo": "cd ./src/demo && webpack",
    "start-demo": "node ./demo/demo-server.js",
    "demo": "yarn build-demo && yarn start-demo && yarn open",
    "inject-css": "gulp copy-css && gulp inject-css",
    "open": "open http://localhost:3000"
  },
  "files": [
    "dist",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://djorg83@github.com/djorg83/react-bootstrap-sweetalert.git"
  },
  "keywords": [
    "react",
    "bootstrap",
    "alert",
    "sweetalert",
    "confirm",
    "modal"
  ],
  "author": "Daniel Jorgensen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djorg83/react-bootstrap-sweetalert/issues"
  },
  "homepage": "https://github.com/djorg83/react-bootstrap-sweetalert#readme",
  "peerDependencies": {
    "prop-types": ">=15.0.0",
    "react": ">=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^12.7.1",
    "@types/react": "^16.9.1",
    "@types/react-bootstrap": "^0.32.19",
    "@types/react-dom": "^16.8.5",
    "css-loader": "^3.2.0",
    "express": "^4.17.1",
    "gulp": "^4.0.2",
    "gulp-inject-file": "0.0.19",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.9.0",
    "react-tools": "^0.13.3",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {}
}
